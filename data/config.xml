<?xml version="1.0" encoding="UTF-8" standalone="no"?>


<game>
    <frame width="1200" height="700" padding="16" border="7" title="Tetris" sizeRol="5">
        <Layer className="ui.ui.LayerAbout" x="0" y="0" w="0" h="0"/>
        <Layer className="ui.ui.LayerBackGround" x="0" y="0" w="0" h="0"/>
        <Layer className="ui.ui.LayerButton" x="788" y="32" w="334" h="124"/>
        <Layer className="ui.ui.LayerDataBase" x="40" y="32" w="334" h="279"/>
        <Layer className="ui.ui.LayerDisk" x="40" y="343" w="334" h="279"/>
        <Layer className="ui.ui.LayerGame" x="414" y="32" w="334" h="590"/>
        <Layer className="ui.ui.LayerLevel" x="964" y="188" w="158" h="148"/>
        <Layer className="ui.ui.LayerNext" x="788" y="188" w="176" h="148"/>
        <Layer className="ui.ui.LayerPoint" x="788" y="368" w="334" h="200"/>
    </frame>

    <data>
        <dataBase className="dao.DataBase">
            <param key="driver" value="com.mysql.jdbc.Driver"/>
            <param key="dbUrl" value="jdbc:mysql://127.0.0.1:3306/?useSSL=false"/>
            <param key="dbUser" value="tetris"/>
            <param key="dbPwd" value="tetrisdbpass"/>
        </dataBase>
        <dataDisk className="dao.DataDisk">
            <param key="filePath" value="./data/recode.dat"/>
        </dataDisk>
    </data>

    <system >
        <rect round="true">
            <point x="4" y="0"/>
            <point x="3" y="0"/>
            <point x="5" y="0"/>
            <point x="6" y="0"/>

        </rect>
        <rect round="true">
            <point x="4" y="0"/>
            <point x="3" y="0"/>
            <point x="5" y="0"/>
            <point x="4" y="1"/>

        </rect>
        <rect round="true">
            <point x="4" y="0"/>
            <point x="3" y="0"/>
            <point x="5" y="0"/>
            <point x="3" y="1"/>

        </rect>
        <rect round="true">
            <point x="4" y="0"/>
            <point x="5" y="0"/>
            <point x="3" y="1"/>
            <point x="4" y="1"/>

        </rect>
        <rect round="false">
            <point x="4" y="0"/>
            <point x="5" y="0"/>
            <point x="4" y="1"/>
            <point x="5" y="1"/>

        </rect>
        <rect round="true">
            <point x="4" y="0"/>
            <point x="3" y="0"/>
            <point x="5" y="0"/>
            <point x="5" y="1"/>

        </rect>
        <rect round="true">
            <point x="4" y="0"/>
            <point x="3" y="0"/>
            <point x="4" y="1"/>
            <point x="5" y="1"/>

        </rect>
    </system>

</game>